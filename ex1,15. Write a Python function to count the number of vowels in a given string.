def count_vowels(string):
    vowels = {'a', 'e', 'i', 'o', 'u', 'A', 'E', 'I', 'O', 'U'}
    
    count = 0
    
    for char in string:
        if char in vowels:
            count += 1
    
    return count

input_string = input("Enter a string: ")

vowel_count = count_vowels(input_string)

print("Number of vowels in the string:", vowel_count)
